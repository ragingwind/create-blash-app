{
  "extends": "./base.json",
  "compilerOptions": {
    "rootDirs": [
      "./src",
      "../.redwood/types/mirror/web/src",
      "../api/src",
      "../.redwood/types/mirror/api/src"
    ],
    "paths": {
      "src/*": [
        "./src/*",
        "../.redwood/types/mirror/web/src/*",
        "../api/src/*",
        "../.redwood/types/mirror/api/src/*"
      ],
      "types/*": [
        "./types/*",
        "../types/*"
      ],
      "@redwoodjs/testing": [
        "../node_modules/@redwoodjs/testing/web"
      ]
    },
    "typeRoots": [
      "../node_modules/@types",
      "./node_modules/@types"
    ],
    "types": [
      "jest",
      "@testing-library/jest-dom"
    ],
    "jsx": "preserve"
  }
}